{
  "name": "trello",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn build:style && react-scripts start",
    "build": "yarn build:style && react-scripts build",
    "build:style": "tailwind build src/styles/index.css -o src/styles/tailwind.css",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "e2e": "start-test start 3000 cy:run",
    "storybook": "start-storybook -p 9009 -s public",
    "sb": "yarn storybook",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@sentry/browser": "^5.11.0",
    "apollo-boost": "^0.4.4",
    "autoprefixer": "^9.7.1",
    "aws-amplify": "^1.2.4",
    "aws-amplify-react": "^2.5.4",
    "formik": "^2.0.4",
    "graphql": "^14.5.8",
    "postcss-cli": "^6.1.3",
    "react": "^16.11.0",
    "react-beautiful-dnd": "^12.0.0",
    "react-dom": "^16.11.0",
    "react-helmet-async": "^1.0.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-spring": "^8.0.27",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1",
    "styled-components": "^4.4.1",
    "tailwindcss": "^1.1.3",
    "typescript": "^3.7.2",
    "uuid": "^3.3.3",
    "whatwg-fetch": "^3.0.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-centered": "^5.2.8",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.7",
    "@types/react": "16.9.11",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "^5.0.14",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "^4.4.0",
    "@types/uuid": "^3.4.6",
    "@types/yup": "^0.26.27",
    "cypress": "^3.8.0",
    "start-server-and-test": "^1.10.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
